{
	"type": "module",
	"name": "pause-ai",
	"private": true,
	"packageManager": "pnpm@9.14.4",
	"scripts": {
		"clean": "tsx scripts/clean.ts",
		"inlang:settings": "tsx scripts/inlang-settings.ts",
		"dev": "tsx scripts/l10n/run --dryRun && vite dev --host 0.0.0.0",
		"build": "cross-env NODE_ENV=production node scripts/filter-build-log.js \"tsx scripts/l10n/run && vite build --emptyOutDir=false && run-s _postbuild:*\"",
		"_postbuild:pagefind": "tsx scripts/create-pagefind-index.ts",
		"_postbuild:l10ntamer": "tsx scripts/l10ntamer.ts",
		"test": "vitest",
		"preview": "vite preview",
		"check": "run-s _check:*",
		"_check:svelte": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"_check:scripts": "tsc --noEmit -p scripts.tsconfig.json",
		"check:watch": "echo This commands only checks the SvelteKit code, not the build scripts. && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "knip && eslint .",
		"format": "prettier --write .",
		"prepare": "husky",
		"sync": "svelte-kit sync"
	},
	"devDependencies": {
		"@eslint/js": "9.35.0",
		"@eslint/markdown": "7.2.0",
		"@inlang/paraglide-js": "2.3.2",
		"@sveltejs/adapter-node": "5.4.0",
		"@sveltejs/kit": "2.42.1",
		"@sveltejs/vite-plugin-svelte": "3.1.2",
		"@types/escape-html": "1.0.4",
		"@types/glidejs__glide": "3.6.6",
		"@types/minimist": "1.2.5",
		"@types/node": "24.5.2",
		"@types/remark-heading-id": "1.0.0",
		"@types/ua-parser-js": "0.7.39",
		"axios-retry": "4.5.0",
		"cross-env": "7.0.3",
		"dotenv": "16.6.1",
		"eslint": "9.35.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-svelte": "3.12.3",
		"globals": "16.4.0",
		"husky": "9.1.7",
		"knip": "5.63.1",
		"lint-staged": "15.5.2",
		"mdsvex": "0.11.2",
		"minimatch": "9.0.5",
		"minimist": "1.2.8",
		"npm-run-all2": "6.2.6",
		"p-queue": "8.1.1",
		"prettier": "3.6.2",
		"prettier-plugin-svelte": "3.4.0",
		"remark-heading-id": "1.0.1",
		"remove-markdown": "0.5.5",
		"simple-git": "3.28.0",
		"svelte": "4.2.20",
		"svelte-check": "4.3.1",
		"tsx": "4.20.5",
		"typescript": "~5.8.3",
		"typescript-eslint": "8.44.0",
		"vite": "5.4.20",
		"vite-plugin-lucide-preprocess": "1.4.4",
		"vitest": "2.1.9"
	},
	"dependencies": {
		"@anthropic-ai/sdk": "0.39.0",
		"@fontsource/roboto-slab": "5.2.8",
		"@fontsource/saira-condensed": "5.2.8",
		"@glidejs/glide": "~3.6.2",
		"@pagefind/default-ui": "1.4.0",
		"@prgm/sveltekit-progress-bar": "2.0.0",
		"@remix-run/headers": "0.12.0",
		"@sveltejs/enhanced-img": "~0.3.10",
		"@turf/distance": "7.2.0",
		"airtable": "0.12.2",
		"axios": "1.12.2",
		"clipboard-polyfill": "4.1.1",
		"escape-html": "1.0.3",
		"github-slugger": "2.0.0",
		"html-to-image": "1.11.13",
		"http-status-codes": "2.3.0",
		"lucide-svelte": "0.536.0",
		"maplibre-gl": "5.7.2",
		"maplibregl-mapbox-request-transformer": "0.0.2",
		"micromark": "4.0.2",
		"pagefind": "1.4.0",
		"rehype-slug": "5.1.0",
		"remark-toc": "9.0.0",
		"remark-unwrap-images": "3.0.1",
		"snakecase-keys": "8.1.0",
		"svelte-french-toast": "1.2.0",
		"svelte-markdown": "0.4.1",
		"svelte-toc": "0.5.9",
		"ua-parser-js": "1.0.41"
	}
}
